<div>
	<%= link_to "Sign up", signup_path %>
	<div>
		<%= form_for(:session, url: '') do |f| %>

			<%= f.label :email %>
			<%= f.text_field :email %>

			<%= f.label :password %>
			<%= f.password_field :password %>

			<%= f.submit "Log in" %>
		<% end %>
	</div>
</div>

